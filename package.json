{
  "name": "brand_registration",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "generate:css": "tailwindcss -i ./css/input.css -o ./css/output.css",
    "watch:css": "tailwindcss -i ./css/input.css -o ./css/output.css --watch",
    "build": "gulp",
    "live": "live-server",
    "live:optimized": "live-server --open=/build --mount=/css:/build/css --mount=/js:/build/js  --mount=/html:/build/html",
    "start": "concurrently \"npm run watch:css\" \"npm run live\"",
    "start:optimized": "concurrently \"npm run build\" \"npm run live:optimized\""
  },
  "author": "Usman Jamil",
  "license": "ISC",
  "dependencies": {
    "tailwindcss": "^3.4.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "gulp": "^5.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-replace": "^1.1.4",
    "gulp-uglify": "^3.0.2",
    "live-server": "^1.2.2"
  }
}
